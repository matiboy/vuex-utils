{
  "name": "vuex-utils",
  "version": "1.0.10",
  "description": "Utils for Vuex common operations",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "./node_modules/.bin/nyc mocha --require babel-core/register --colors './{,!(node_modules)/**/}*.test.js'",
    "test:watch": "./node_modules/.bin/nyc mocha --require babel-core/register --colors -w ./**/*.test.js",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matiboy/vuex-utils.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Matiboy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matiboy/vuex-utils/issues"
  },
  "homepage": "https://github.com/matiboy/vuex-utils",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.12.0",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "nyc": "^11.4.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "lodash": "^4.17.4"
  }
}
